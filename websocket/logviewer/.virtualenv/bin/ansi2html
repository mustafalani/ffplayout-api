#!/opt/ffplayout-engine/websocket/logviewer/.virtualenv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ansi2html==1.2.0','console_scripts','ansi2html'
__requires__ = 'ansi2html==1.2.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ansi2html==1.2.0', 'console_scripts', 'ansi2html')()
    )
